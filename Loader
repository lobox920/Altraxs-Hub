function Loading()
    rconsoleprint("\n Loading in process please wait ")
end
spawn(Loading)
task.wait(0.8)
function updatecheck()
    if game.PlaceId == 2534724415 then
     if game.PlaceVersion ~= 2265 then 
        game.Players.LocalPlayer:Kick("\n For more information report to https://discord.gg/3dPPvsVAy4 or Awaken#6969 & lobox920#9889 ") 
         rconsoleprint("\n  ","Game Updated, New version is " ..game.PlaceVersion.." Please report that to owners")
        wait(0.2)
        game:Shutdown()
       elseif game.PlaceVersion == 2265 then
        rconsoleprint("\n  Altrix Hub : ".."Script Updated, Current Game Version:"..game.PlaceVersion)
        rconsoleprint("\n  Altrix Hub : " .. " Welcome "..game:GetService("Players").LocalPlayer.Name.."Make sure you give your feedback in discord")
      end
    elseif game.PlaceId == 9210611498 then
     if game.PlaceVersion ~= 6468 then 
        game.Players.LocalPlayer:Kick("\n For more information report to https://discord.gg/3dPPvsVAy4 or Awaken#6969 & lobox920#9889 ") 
        rconsoleprint("\n  ","Game Updated, New version is "..game.PlaceVersion.."Please report that to owners")
        wait(0.2)
        game:Shutdown()
     elseif game.PlaceVersion == 6468 then
        rconsoleprint("\n  Altrix Hub :".." Script Updated, Current Game Version: "..game.PlaceVersion)
        rconsoleprint("\n  Altrix Hub :" .. " Welcome "..game:GetService("Players").LocalPlayer.Name.."Make sure you give your feedback in discord")
      end
    end
end
function acbypass()
   local BlockedRemotes = { 
      "ResyncCharacter",
      "Handcuffs",
      "CarLeave",
      "GetCurrency", 
      "GameAnalyticsError",
      "PurchaseShopItem",
      "ToolPickUp",
      "CarModify",
      "CheckWalkSpeed",
      "ViewTrafficCamera",
  }
  local Events = {
      Fire = true, 
      Invoke = true, 
      FireServer = true, 
      InvokeServer = true,
  }
  
  local gameMeta = getrawmetatable(game)
  local psuedoEnv = {
      ["__index"] = gameMeta.__index,
      ["__namecall"] = gameMeta.__namecall;
  }
  setreadonly(gameMeta, false)
  gameMeta.__index, gameMeta.__namecall = newcclosure(function(self, index, ...)
      local scripts = getcallingscript()
      if Events[index] then
          for i,v in pairs(BlockedRemotes) do
              if v == self.Name and not checkcaller() then
                  return workspace:WaitForChild("Altrax Hub") -- It is supposed to send a function and error with a WaitForChild
              end
          end
      end
      return psuedoEnv.__index(self, index, ...)
  end)
  setreadonly(gameMeta, true)
  local ContentProvider = game:GetService("ContentProvider")
  hookfunction(ContentProvider.PreloadAsync or ContentProvider.PreloadAsync(), function(...) 
      return workspace:WaitForChild("Altrax Hub") -- Some dex or stuff getting detected by this
  end)
end
spawn(updatecheck)
task.wait(0.8)

